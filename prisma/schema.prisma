generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AuthUser {
  id                Int       @id @default(autoincrement())
  password          String    @db.VarChar(128)
  last_login        DateTime? @db.Timestamp(6)
  is_processor      Int?      @db.SmallInt
  username          String    @unique @db.VarChar(150)
  first_name        String?   @db.VarChar(156)
  last_name         String?   @db.VarChar(159)
  email             String?   @db.VarChar(254)
  is_addr           Int?      @db.SmallInt
  is_active         Int?      @db.SmallInt
  date_ponse        DateTime? @db.Timestamp(6)
  palabras_seguridad String[]  @db.VarChar(50)
  groups            AuthUserGroup[]
  permissions       AuthUserPermission[]
  cree_wet_podificacion CreeWetPodificacion[]
}

model AuthGroup {
  id        Int       @id @default(autoincrement())
  name      String    @unique @db.VarChar(156)
  users     AuthUserGroup[]
}

model AuthUserGroup {
  id        BigInt    @id @default(autoincrement())
  user_id   Int
  group_id  Int
  user      AuthUser  @relation(fields: [user_id], references: [id])
  group     AuthGroup @relation(fields: [group_id], references: [id])
}

model AuthUserPermission {
  id            BigInt @id @default(autoincrement())
  user_id       Int
  permission_id Int
  user          AuthUser @relation(fields: [user_id], references: [id])
}

model Usuaries {
  usuario         Int      @id
  nombre_usuario  String?  @db.VarChar(100)
  usuario_varchar String?  @db.VarChar(15)
  ficha_ingreso   String?  @db.VarChar(45)
  dirino_ingreso  String?  @db.VarChar(45)
  foto_central    String?  @db.VarChar(100)
  list_login      DateTime? @db.Timestamp(6)
  password        String?  @db.VarChar(128)
  clave_usuario   String?  @db.VarChar(100)
}

model BienesFacturacion {
  id_facturacion   Int      @id @default(autoincrement())
  numero_inventario String?  @db.VarChar(100)
  nombre           String?  @db.VarChar(100)
  marca            String?  @db.VarChar(100)
  modelo           String?  @db.VarChar(100)
  serial           String?  @db.VarChar(100)
  color            String?  @db.VarChar(100)
  directory        String?  @db.VarChar(100)
  ficha_ingreso    String?  @db.VarChar(45)
  foto1            String?  @db.VarChar(100)
  foto2            String?  @db.VarChar(100)
  id_coordination_id Int?
}

model CreeWetPodificacion {
  id              BigInt    @id @default(autoincrement())
  message         String?   @db.Text
  leido           Int?      @db.SmallInt
  ficha_creador   DateTime? @db.Timestamp(6)
  autorizado      Int?      @db.SmallInt
  usuario_id      Int
  user            AuthUser  @relation(fields: [usuario_id], references: [id])
}

model Vigilantes {
  id_vigilante     Int      @id @default(autoincrement())
  numero_inventario String?  @db.VarChar(100)
  nombre_ben       String?  @db.VarChar(100)
  destino_ben      String?  @db.VarChar(100)
  hora_entrada     String?  @db.VarChar(45)
  hora_salida      String?  @db.VarChar(45)
  ficha_retorno    String?  @db.VarChar(100)
  foto1            String?  @db.VarChar(100)
  foto2            String?  @db.VarChar(100)
  id_coordination_id Int?
}

model BienesTransitorios {
  id_transitorio   Int      @id @default(autoincrement())
  numero_inventario String?  @db.VarChar(100)
  nombre           String?  @db.VarChar(100)
  ficha_ingreso    String?  @db.VarChar(45)
  foto             String?  @db.VarChar(100)
  id_coordination_id Int?
}

model CrearCoordinacion {
  id_coordinacion  Int      @id @default(autoincrement())
  codigo           String?  @db.VarChar(45)
  nombre           String?  @db.VarChar(45)
  description      String?  @db.VarChar(45)
}

model Bienes {
  id_bienes        Int      @id @default(autoincrement())
  numero_inventario String?  @db.VarChar(100)
  nombre_ben       String?  @db.VarChar(100)
  marca            String?  @db.VarChar(100)
  modelo           String?  @db.VarChar(100)
  serial           String?  @db.VarChar(100)
  caracteristica   String?  @db.VarChar(100)
  codigo_color     String?  @db.VarChar(45)
  color_ubica_trimestria String?  @db.VarChar(100)
  estado_colos     String?  @db.VarChar(15)
  foto1            String?  @db.VarChar(100)
  foto2            String?  @db.VarChar(100)
  id_coordination_id Int?
}